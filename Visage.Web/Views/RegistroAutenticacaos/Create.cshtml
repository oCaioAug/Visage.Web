@model Visage.Web.Models.RegistroAutenticacao

@{
    ViewData["Title"] = "Adicionar Registro de Autenticação";
}

<h1>@ViewData["Title"]</h1>

<h4>RegistroAutenticacao</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="UsuariosId" class="control-label"></label>
                <select asp-for="UsuariosId" class ="form-control" asp-items="ViewBag.UsuariosId"></select>
            </div>
            <div class="form-group">
                <label asp-for="DataHora" class="control-label"></label>
                <input asp-for="DataHora" class="form-control" />
                <span asp-validation-for="DataHora" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Status" class="control-label"></label>
                <input asp-for="Status" class="form-control" />
                <span asp-validation-for="Status" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TipoAcesso" class="control-label"></label>
                <input asp-for="TipoAcesso" class="form-control" />
                <span asp-validation-for="TipoAcesso" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
